-- Load Rayfield
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- Create Window
local Window = Rayfield:CreateWindow({
   Name = "29yumoos Hub | Beta",
   Icon = 0,
   LoadingTitle = "29yumoos Hub Loading...",
   LoadingSubtitle = "by 29yumoos",
   ShowText = "29yumoos Hub",
   Theme = "Default",

   ToggleUIKeybind = "K",

   ConfigurationSaving = {
      Enabled = true,
      FolderName = "29yumoosHub",
      FileName = "29yumoosBeta"
   },

   Discord = {
      Enabled = true,
      Invite = "5rYVafZbdX", -- Your Discord invite
      RememberJoins = true
   },

   KeySystem = true, -- ✅ Rayfield Key System
   KeySettings = {
      Title = "29yumoos Hub | Key System",
      Subtitle = "Beta Access",
      Note = "Get the key at: https://link-hub.net/1399417/gKVh8EpemsYM",
      FileName = "29yumoosKey",
      SaveKey = true,
      GrabKeyFromSite = false,
      Key = {"Iwasboredsoimakethehub", "99yumoos"}
   }
})

-- Tabs
local MainTab = Window:CreateTab("Main", 4483362458)
local ScriptsTab = Window:CreateTab("Scripts", 4483362458)

-- Main Section
local MainSection = MainTab:CreateSection("Welcome to 29yumoos Hub")

-- Scripts Section
local DexButton = ScriptsTab:CreateButton({
   Name = "Dark Dex V3",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/BypassedDarkDexV3.lua", true))()
   end,
})

local PShadeButton = ScriptsTab:CreateButton({
   Name = "PShade Ultimate",
   Callback = function()
      loadstring(game:HttpGet('https://raw.githubusercontent.com/randomstring0/pshade-ultimate/refs/heads/main/src/cd.lua'))()
   end,
})

local InfiniteYieldButton = ScriptsTab:CreateButton({
   Name = "Infinite Yield Admin",
   Callback = function()
      loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
   end,
})

local ComingSoonButton = ScriptsTab:CreateButton({
   Name = "⚡ More Scripts Coming Soon...",
   Callback = function()
      Rayfield:Notify({
         Title = "Coming Soon!",
         Content = "Stay tuned for more scripts in 29yumoos Hub.",
